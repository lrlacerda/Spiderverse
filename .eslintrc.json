{
    "extends": ["next/core-web-vitals", "prettier"],
    "plugins": ["unused-imports", "import"],
    "rules": {
        "unused-imports/no-unused-imports": "off", // Desativa essa regra globalmente
        "import/order": ["error", { "newlines-between": "always" }],
        "no-unused-vars": "off"
    }
}
